{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es5",
      "es2015.iterable",
      "es2015.generator",
      "es2015.symbol"]
  },
  "exclude": [
    "node_modules"
  ]
}